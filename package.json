    "morgan": "^1.7.0",
    "node-fetch": "^2.6.0",
    "opentracing": "^0.14.4",
    "prom-client": "^11.5.3",
    "raven": "^2.2.1",
    "request": "^2.81.0",
    "stack-trace": "^0.0.9",
