    "@opentelemetry/instrumentation-express": "^0.16.0",
    "@opentelemetry/instrumentation-http": "^0.19.0",
    "@opentelemetry/instrumentation-ioredis": "^0.16.0",
    "@opentelemetry/node": "^0.19.0",
    "@opentelemetry/tracing": "^0.19.0",
    "async": "^2.0.1",
    "body-parser": "^1.15.2",
    "deepcopy": "^0.6.3",
